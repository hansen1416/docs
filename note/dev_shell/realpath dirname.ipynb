    "# go one level up\n",
    "parent_dir=$(dirname $dir_path)\n",
    "echo $parent_dir\n",
    "\n",
    "# or this way\n",
    "p_dir=$(dirname $(dirname $(dirname $(realpath $0))))\n",
    "echo $p_dir"
